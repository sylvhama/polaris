{
  "name": "@shopify/polaris-cli",
  "version": "0.0.0",
  "private": false,
  "description": "Shopify CLI plugin for managing Polaris in Shopify apps",
  "bugs": {
    "url": "https://github.com/Shopify/cli/issues"
  },
  "homepage": "https://github.com/shopify/cli#readme",
  "type": "module",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "tsc -b ./tsconfig.build.json",
    "prepack": "cross-env NODE_ENV=production yarn run build && cp ../../README.md README.md",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "tsc": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "../../.eslintrc.cjs"
    ]
  },
  "dependencies": {
    "@oclif/core": "1.9.2",
    "@shopify/cli-kit": "3.8.0",
    "@shopify/polaris-migrator": "^0.0.0",
    "@shopify/prettier-config": "^1.1.2",
    "@types/prettier": "^2.6.3",
    "prettier": "^2.6.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.9"
  },
  "devDependencies": {
    "vitest": "^0.17.1"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=14.13.1"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "oclif": {
    "commands": "dist/cli/commands"
  }
}
